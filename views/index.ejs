<html lang="en">
<head>
    <title>Homebeasts - Discover your dream home at the lowest cost</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon" href="./img/beast-favicon.png" type="image/gif">
    <link rel="stylesheet" href="./bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="./custom.css">
    <link href="./fontawesome-free-5.14.0-web/css/all.css" rel="stylesheet">
    <script src="./jquery.min.js"></script>
    <script src="./popper.min.js"></script>
    <script src="./bootstrap.min.js"></script>
    

</head>
<body>
    <nav class="custom navbar navbar-expand-sm navbar-light">
        <ul class="navbar-nav">
            <li class="nav-item">
                <a class="custom nav-link logo" href="/">Homebeasts</a>
            </li>
            <li class="nav-item">
            <a class="nav-link" href="#" style="margin-top: 5px;">Link</a>
            </li>
            <li class="nav-item">
            <a class="nav-link" href="#" style="margin-top: 5px;">Disabled</a>
            </li>
        </ul>
        <div class="custom navbar-collapse collapse w-100 order-3 dual-collapse2">
            <ul class="navbar-nav ml-auto">
                <li class="nav-item">
                    <a href="/signin" role="button" class="btn sign-in">Sign in</a>
                </li>
                <li class="nav-item">
                    <a href="/signup" role="button" class="btn sign-up">Sign up</a>
                </li>
            </ul>
        </div>
    </nav>

    <div class="container signed-out-masthead">
        <div class="row">
            <div class="signed-out-masthead-short-disc col-sm-6">
                    <p class="slogan">Discovert your dream home at the lowest cost</p>
                    <p class="discription">Homebeasts helps you find your perfect home that suits your budget and connect with our top and creative architects all over the Philippines.</p>
                    <a href="/signup" role="button" class="btn sign-up">Sign up</a>
            </div>
            <div class="signed-out-masthead-art col-sm-6">
            </div>
          </div>
    </div>

    <div class="container main-content">
        <nav class="custom navbar navbar-expand-md navbar-light">
            <div class="navbar-collapse collapse w-100 order-1 order-md-0 dual-collapse2">
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item active">
                        <button type="button" class="btn saved">Saved
                            <i class="material-icons add_box">add_box</i>
                        </button>
                    </li>
                </ul>
            </div>
            <div class="mx-auto order-0">
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item category-menu active">
                        <a class="nav-link" href="#">All</a>
                    </li>
                    <li class="nav-item category-menu">
                        <a class="nav-link" href="#">Popular</a>
                    </li>
                    <li class="nav-item category-menu">
                        <a class="nav-link" href="#">Condo</a>
                    </li>
                    <li class="nav-item category-menu">
                        <a class="nav-link" href="#">Apartment</a>
                    </li>
                    <li class="nav-item category-menu">
                        <a class="nav-link" href="#">Subdivision</a>
                    </li>
                    <li class="nav-item category-menu">
                        <a class="nav-link" href="#">House&Lot</a>
                    </li>
                    <li class="nav-item category-menu">
                        <a class="nav-link" href="#">Architecture</a>
                    </li>
                </ul>
            </div>
            <div class="custom navbar-collapse collapse w-100 order-3 dual-collapse2">
                <ul class="navbar-nav ml-auto">
                    <li class="nav-item">
                        <button type="button" class="btn filters"><i class="material-icons filter_list">filter_list</i>Filters</button>
                    </li>
                </ul>
            </div>
        </nav>
        <div class="custom-cards row">
            <div class="col-md-3">
                <div class="custom card">
                    <img class="card-img-top" src="./img/4d5f23dda58cd9c7f242eb3d00368ece.jpg" alt="Card image">
                    <div class="card-img-overlay">
                        <i class="material-icons collections hide-icon">collections</i>
                        <div class="overlay-content hide-overlay">
                            <p class="card-title">House & Lot | Subdivision</p>
                            <div class="card-icons">
                                <a href="#" class="btn btn-light material-icons">add_box</a>
                                <a href="#" class="btn btn-light material-icons favorite">favorite</a>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="card-short-info row">
                    <div class="avatar-name col-md-8">
                        <img src="./img/camella-homes.png" style="border-radius:100%;" width="28" height="28">
                        <p class="diplay-name">Camella Homes Cebu</p>
                    </div>
                    
                    <div class="short-info-icons col-md-4">
                            <span class="material-icons textsms">textsms</span>
                            <span class="comments">13</span>
                            <span class="material-icons favorite">favorite</span>
                            <span class="reacts">120</span>
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="card">
                    <div class="card-body">Basic card</div>
                  </div>
            </div>
            <div class="col-md-3">
                <div class="card">
                    <div class="card-body">Basic card</div>
                  </div>
            </div>
            <div class="col-md-3">
                <div class="card">
                    <div class="card-body">Basic card</div>
                  </div>
            </div>
        </div>
    </div>
<script>
    $(function() {
        $(".category-menu").click(function() {
            
            $(".category-menu").removeClass("active");
            
            $(this).addClass("active");
        });
    });
</script>
</body>
</html>